{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cargo run",
			"type": "shell",
			"command": "./script/run.sh",
			"args": [
				"${relativeFile}"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared"
			},
			"problemMatcher": "$rustc"
		}
	]
}
